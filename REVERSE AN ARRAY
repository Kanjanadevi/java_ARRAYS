import java.util.*;
public class Main{
public static void main (String[] args) {
    Scanner sc = new Scanner(System.in);
    int n= sc.nextInt();
    int arr[] = new int[n];
    for (int i=0;i<n;i++){
        arr[i]=sc.nextInt();
    }
    for(int i = n - 1; i >= 0; i--) {
        System.out.print(arr[i] + " ");
     }
}
    
}

input:
4
8 7 5 3 

output:
3 5 7 8 
